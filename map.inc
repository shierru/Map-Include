 * <library>map</library>
 */
stock MAPZONE:GetPlayerMapZone3D(playerid)
{
	new Float:x = 0.0, 
        Float:y = 0.0, 
        Float:z = 0.0;

	if(!GetPlayerPos(playerid, x, y, z))
		return INVALID_MAPZONE_ID;

	return GetMapZoneAtPoint3D(x, y, z);
