{
	return MAPZONE_DATA_SIZE;
}

/**
 * <library>map</library>
 */
stock bool:IsValidMapZone(MAPZONE:index)
{
	return (0 <= _:index < MAPZONE_DATA_SIZE);
}

/**
 * <library>map</library>
 */
stock bool:GetMapZoneName(MAPZONE:index, output[], size = sizeof(output)) 
{
	if(!IsValidMapZone(index))
		return false;

	strunpack(output, s_MapZonesData[_:index][MAPZONE_NAME], size);

	return true;
}

/**
 * <library>map</library>
 */
stock bool:GetMapZoneSoundID(MAPZONE:index, &soundid) 
{
	if(!IsValidMapZone(index))
		return false;

	soundid = s_MapZonesData[_:index][MAPZONE_SOUND_ID];

	return true;
}

/**
 * <library>map</library>
 */
stock bool:GetMapZoneAreaCount(MAPZONE:index, &count)
{
	if(!IsValidMapZone(index))
		return false;

	count = s_MapZonesData[_:index][MAPZONE_AREA_COUNT];

	return true;
}

/**
 * <library>map</library>
 */
stock GetMapZoneAreaPos(MapZone:id, &Float:minX = 0.0, &Float:minY = 0.0, &Float:minZ = 0.0, &Float:maxX = 0.0, &Float:maxY = 0.0, &Float:maxZ = 0.0, start = 0) 
{
	if(!IsValidMapZone(id) || start < 0)
		return -1;

	for(new size = MAPZONE_AREA_DATA_SIZE; start < size; start++)
    {
        if(s_MapZonesAreaData[start][MAPZONE_AREA_ID] != id)
            continue;

		minX = s_MapZonesAreaData[start][MAPZONE_AREA_MIN_X];
        minY = s_MapZonesAreaData[start][MAPZONE_AREA_MIN_Y];
        minZ = s_MapZonesAreaData[start][MAPZONE_AREA_MIN_Z];

        maxX = s_MapZonesAreaData[start][MAPZONE_AREA_MAX_X];
        maxY = s_MapZonesAreaData[start][MAPZONE_AREA_MAX_Y];
        maxZ = s_MapZonesAreaData[start][MAPZONE_AREA_MAX_Z];

        return start;
	}

	return -1;
}